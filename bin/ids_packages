#!/usr/bin/env python
import subprocess
import sys


def install(package):
    subprocess.check_call([sys.executable, "-m", "pip", "install", package])


install('C:/Program Files/IDS/ids_peak/sdk/api/binding/python/wheel/x86_64/ids_peak-1.2.0.5-cp38-cp38-win_amd64.whl')
install('C:/Program Files/IDS/ids_peak/sdk/ipl/binding/python/wheel/x86_64/ids_peak_ipl-1.2.0.5-cp38-cp38-win_amd64.whl')
